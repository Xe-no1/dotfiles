{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "background": "#82b8c8",
          "foreground": "#000000",
          "trailing_diamond": "\ue0b0",
          "leading_diamond": "\ue0b2",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "diamond",
          "template": " \udb80\ude56 {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#aac474",
          "foreground": "#000000",
          "trailing_diamond": "\ue0b0",
          "leading_diamond": "",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .HEAD }} \uf062 {{ .Ahead }} \uf063 {{ .Behind }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#f4bf75",
          "foreground": "#000000",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} \uf252 ",
          "trailing_diamond": "\ue0b0",
          "leading_diamond": "",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#000000",
          "foreground": "#ffffff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ac4242{{ end }}"],
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "background": "#f4bf75",
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }} \uf252",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "\u2570",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\uf061",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .PWD }}",
  "transient_prompt": {
    "foreground": "#ffffff",
    "template": "\u2500\uf061 "
  },
  "final_space": true,
  "version": 2
}
